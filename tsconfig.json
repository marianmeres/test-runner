{
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	],
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"rootDir": "src",
		"outDir": "dist",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"declaration": true,
		"pretty": true,
		"skipLibCheck": true,
		"strict": false
	}
}
